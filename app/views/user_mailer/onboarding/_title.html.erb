<div>
  <div style="padding-bottom:24px;">
    <table style="border-spacing:0;border-collapse:collapse;text-align:left;vertical-align:top;padding:0;width:100%;display:table;">
      <tbody><tr style="padding:0;vertical-align:top;text-align:left;">
        <th style="font-size:16px;text-align:left;line-height:1.3;color:#0a0a0a;font-weight:normal;padding:0;width:564px;margin:0 auto;padding-left:16px;padding-right:16px;padding-bottom:0px!important;">
          <p style="padding:0;margin:0;text-align:left;max-width:485px;font-weight:700;color:#484848;line-height:1.3;font-size:32px;padding-bottom:8px;margin-bottom:0!important;">
            <%= title %>
          </p>
        </th>
      </tr>
    </tbody></table>
    <table style="border-spacing:0;border-collapse:collapse;text-align:left;vertical-align:top;padding:0;width:100%;display:table;">
      <tbody><tr style="padding:0;vertical-align:top;text-align:left;">
        <td style="font-size:16px;text-align:left;line-height:1.3;color:#0a0a0a;font-weight:normal;padding:0;width:564px;margin:0 auto;padding-left:16px;padding-right:16px;padding-bottom:0px!important;">
            <p style="padding:0;margin:0;line-height:1.4;font-weight:300;color:#484848;font-size:18px;text-align:left;margin-bottom:0px!important;">
              <%= text %>
            </p>
        </td>
      </tr>
    </tbody></table>
  </div>
</div>
